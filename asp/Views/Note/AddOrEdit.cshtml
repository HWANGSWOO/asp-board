@model asp.Models.Note

@{
    ViewData["Title"] = "Edit";
   
}



<div class="row">
    <div class="col-md-12">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="NoteNo" />
     
            <div class="form-group">
                <label asp-for="NoteTitle" class="control-label"></label>
                <input asp-for="NoteTitle" class="form-control" />
                <span asp-validation-for="NoteTitle" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NoteContents" class="control-label"></label>
                <input asp-for="NoteContents" class="form-control" />
                <span asp-validation-for="NoteContents" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="수정" class="btn btn-primary" />
                <a class="btn btn-outline-primary" asp-action="Index">목록</a>
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
